{
  "name": "ctx-router",
  "version": "1.0.1",
  "description": "Context Router",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/example/express.ts",
    "format": "prettier --write src",
    "format:staged": "lint-staged --config .lintstagedrc.json --allow-empty",
    "lint": "eslint --fix src",
    "lint:staged": "lint-staged --config .lintstagedrc.json --allow-empty",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyugorix/ctx-router.git"
  },
  "keywords": [
    "ctx",
    "router",
    "context"
  ],
  "author": "Kaushik R Bangera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyugorix/ctx-router/issues"
  },
  "homepage": "https://github.com/hyugorix/ctx-router#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
